{
  "expo": {
    "web": {
      "bundler": "metro",
      "favicon": "./assets/adaptive_icon.png"
    },
    "plugins": [
      "@config-plugins/react-native-blob-util",
      "@config-plugins/react-native-pdf",
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "31.0.0"
          },
          "ios": {
            "deploymentTarget": "13.0",
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "name": "cw",
    "slug": "cw",
    "version": "1.0.0",
    "scheme": "com.cloftwaretech.cw",
    "orientation": "portrait",
    "icon": "./assets/adaptive_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://expo-development-client/?url=http%3A%2F%2F192.168.31.82%3A8081"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.cloftwaretech.cw",
      "buildNumber": "1.0.0",
      "entitlements": {
        "com.apple.developer.applesignin": ["Default"],
        "com.apple.developer.networking.wifi-info": true
      }
    },
    "android": {
      "package": "com.cloftwaretech.cw",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive_icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "cafecd33-67fe-4658-a41f-d97dbe01feab"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
