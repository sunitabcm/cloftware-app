{
  "expo": {
    "web": {
      "bundler": "metro",
      "favicon": "./assets/adaptive_icon.png"
    },
    "plugins": [
      "@config-plugins/react-native-blob-util",
      "@config-plugins/react-native-pdf",
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "0.0.10",
            "minSdkVersion": 26
          },
          "ios": {
            "deploymentTarget": "13.0",
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "name": "DPS Ajmer",
    "slug": "cw",
    "version": "0.0.1",
    "scheme": "cloftware.dpss13",
    "orientation": "portrait",
    "icon": "./assets/adaptive_icon.png",
    "userInterfaceStyle": "light",
    "notification": {
      "icon": "./assets/adaptive_icon.png",
      "color": "#ffffff"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://expo-development-client/?url=http%3A%2F%2F192.168.31.82%3A8081"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "cloftware.dpss13",
      "buildNumber": "0.0.1",
      "entitlements": {
        "com.apple.developer.applesignin": ["Default"],
        "com.apple.developer.networking.wifi-info": true
      },
      "permissions": {
        "camera": {
          "description": "Allow app to use camera"
        },
        "mediaLibrary": {
          "description": "Allow app to access media library"
        }
      }
    },
    "android": {
      "package": "cloftware.dpss13",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive_icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": ["CAMERA", "READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"],
      "minSdkVersion": 26
    },
    "extra": {
      "eas": {
        "projectId": "cafecd33-67fe-4658-a41f-d97dbe01feab"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
